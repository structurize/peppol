{
    "name": "structurize/peppol",
    "description": "Laravel package for sending invoices over peppol using Structurize",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Structurize\\Peppol\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Ashim De Coster",
            "email": "ashim@deltasolutions.be"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Structurize\\Peppol\\PeppolServiceProvider"
            ],
            "aliases": {
                "Peppol": "Structurize\\Peppol\\Facades\\Peppol"
            }
        }
    },
    "require": {
        "php": "^7.0|^8.0",
        "illuminate/support": "^5.0|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
        "structurize/structurize": "^0.2.29"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

